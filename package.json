{
  "name": "grapholscape",
  "version": "1.3.0",
  "description": "ontology visualization",
  "module": "build/grapholscape.esm.js",
  "main": "build/grapholscape.js",
  "scripts": {
    "start": "ws -o -d demo & rollup -c -w",
    "build": "cross-env NODE_ENV=production VERSION=$npm_package_version rollup -c",
    "clean": "rimraf ./build",
    "app": "rollup -c && electron ./app",
    "app:dist": "electron-packager ./app --out=./dists --icon=./app/assets/icon --overwrite",
    "deploy": "run-s copy-build-in:demo && cross-env-shell gh-pages -d demo -m \"$npm_package_version\" && rimraf ./demo/js/grapholscape.min.js",
    "doc": "cross-env VERSION=$npm_package_version node doc/doc.js",
    "test": "jest",
    "preversion": "run-s test",
    "version": "run-s doc build app:dist && git add -A"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/obdasystems/grapholscape.git"
  },
  "keywords": [
    "Ontology",
    "OWL",
    "Graphol"
  ],
  "author": "OBDA Systems",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@babel/preset-env": "^7.15.8",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^21.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.0.5",
    "@rollup/plugin-replace": "^3.0.0",
    "babel-jest": "^27.2.5",
    "cross-env": "^7.0.3",
    "electron": "^15.2.0",
    "electron-packager": "^15.4.0",
    "gh-pages": "^3.2.3",
    "jest": "^27.2.5",
    "local-web-server": "^5.1.1",
    "markdox": "^0.1.10",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "rollup": "^2.58.0",
    "rollup-plugin-license": "^2.6.0",
    "rollup-plugin-size-snapshot": "^0.12.0",
    "rollup-plugin-terser": "^7.0.2"
  },
  "dependencies": {
    "@material/mwc-icon": "^0.25.2",
    "@material/mwc-icon-button": "^0.25.2",
    "cytoscape": "^3.19.1",
    "cytoscape-cola": "^2.5.0",
    "cytoscape-popper": "^2.0.0",
    "cytoscape-svg": "^0.3.1",
    "lit": "^2.0.2"
  }
}
