{
  "name": "grapholscape",
  "version": "1.0.0",
  "description": "ontology visualization",
  "main": "app/main.js",
  "scripts": {
    "start": "run-p serve electron:run",
    "build": "rollup -c",
    "watch": "run-p serve watch:build",
    "watch:build": "cross-env FILE=build/grapholscape.js rollup -c -w",
    "serve": "ws",
    "electron-app": "run-s build electron:run",
    "electron:run": "electron ./app",
    "electron:dist": "npx electron-packager ./app --out=./dists --platform=all --icon=./app/assets/icon --overwrite"
  },
  "repository": "https://github.com/obdasystems/grapholscape",
  "keywords": [
    "Ontology",
    "OWL",
    "Graphol"
  ],
  "author": "OBDA Systems",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.3.4",
    "cross-env": "^5.2.0",
    "electron": "^7.0.0",
    "electron-packager": "^14.1.0",
    "livereload": "^0.8.0",
    "local-web-server": "^2.6.1",
    "npm-run-all": "^4.1.5",
    "rollup": "^1.12.3",
    "rollup-plugin-babel": "^4.3.0",
    "rollup-plugin-commonjs": "^9.3.4",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-license": "^0.12.1",
    "rollup-plugin-node-resolve": "^4.2.4",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-size-snapshot": "^0.8.0",
    "rollup-plugin-terser": "^4.0.4",
    "standard": "^12.0.1"
  },
  "dependencies": {
    "@material/mwc-icon": "^0.6.0",
    "@babel/polyfill": "7.4.4",
    "@webcomponents/webcomponentsjs": "^2.3.0",
    "cytoscape": "3.2.22",
    "cytoscape-cola": "^2.3.0",
    "lit-element": "^2.1.0"
  }
}
