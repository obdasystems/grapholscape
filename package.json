{
  "name": "grapholscape",
  "version": "0.0.1",
  "description": "ontology visualization",
  "main": "dist/grapholscape.js",
  "scripts": {
    "start": "run-p serve app",
    "build": "rollup -c && run-s copy-build-in:app",
    "copy-build-in:app": "cpy build/grapholscape.min.js ./app/graphol",
    "copy-build-in:demo": "cpy build/grapholscape.min.js ./demo/js",
    "watch": "run-p serve watch:build",
    "watch:build": "cross-env FILE=grapholscape BABEL=false rollup -c -w",
    "serve": "ws",
    "dist:copy": "cpy build/grapholscape.js build/grapholscape.min.js dist",
    "dist": "cross-env NODE_ENV=production run-s build dist:*",
    "app": "electron ./app",
    "app:dist": "npx electron-packager ./app --out=./dists --platform=all --icon=./app/assets/icon --overwrite",
    "deploy": "run-s copy-build-in:demo && gh-pages -d demo && rimraf ./demo/js/grapholscape.min.js",
    "release": "run-s dist app:dist deploy"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/obdasystems/grapholscape.git"
  },
  "keywords": [
    "Ontology",
    "OWL",
    "Graphol"
  ],
  "author": "OBDA Systems",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.3.4",
    "cpy-cli": "^3.0.0",
    "cross-env": "^5.2.0",
    "electron": "^7.0.0",
    "electron-packager": "^14.1.0",
    "gh-pages": "^2.1.1",
    "livereload": "^0.8.0",
    "local-web-server": "^2.6.1",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.0",
    "rollup": "^1.12.3",
    "rollup-plugin-auto-external": "^2.0.0",
    "rollup-plugin-babel": "^4.3.0",
    "rollup-plugin-commonjs": "^9.3.4",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-license": "^0.12.1",
    "rollup-plugin-node-resolve": "^4.2.4",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-size-snapshot": "^0.8.0",
    "rollup-plugin-terser": "^5.1.3",
    "standard": "^12.0.1"
  },
  "dependencies": {
    "@material/mwc-icon": "^0.6.0",
    "@webcomponents/webcomponentsjs": "^2.3.0",
    "core-js": "^3.6.0",
    "cytoscape": "3.2.22",
    "cytoscape-cola": "^2.3.0",
    "lit-element": "^2.1.0",
    "regenerator-runtime": "^0.13.3"
  }
}
