{
  "name": "grapholscape",
  "version": "4.0.0-snap.7",
  "description": "ontology visualization",
  "module": "dist/grapholscape.esm.js",
  "main": "dist/grapholscape.esm.js",
  "files": [
    "dist"
  ],
  "types": "dist/index.d.ts",
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "scripts": {
    "start": "run-p serve watch",
    "serve": "ws -o -d demo",
    "watch": "rollup -c -w --bundleConfigAsCjs",
    "build": "run-p gen-types build:* && run-s clean:types",
    "build:rollup": "cross-env NODE_ENV=production VERSION=$npm_package_version rollup -c",
    "rollup": "rollup -c",
    "clean": "run-s clean:*",
    "clean:dist": "rimraf ./dist",
    "clean:types": "rimraf ./temp",
    "doc": "run-s doc:*",
    "doc:typedoc": "typedoc",
    "doc:copy-demo-in-doc": "cross-env cpy . ../docs/v$npm_package_version/demo --cwd=demo",
    "test": "jest",
    "gen-types": "run-s clean:types gen-types:tsc",
    "gen-types:tsc": "tsc --declaration --outDir ./temp/types --emitDeclarationOnly",
    "preversion": "run-s test",
    "version": "run-s build doc git-add-all",
    "git-add-all": "git add -A",
    "rdf-graph-gen": "run-s rdf-graph-gen:*",
    "rdf-graph-gen:clean": "rimraf ./src/model/rdf-graph/swagger",
    "rdf-graph-gen:get": "openapi-generator-cli generate -c ./src/model/rdf-graph/config.json -g typescript-fetch -o ./src/model/rdf-graph/swagger -i ./src/model/rdf-graph/rdf-graph.yaml",
    "rdf-graph-gen:fix-index": "node src/model/rdf-graph/fix-index.js",
    "apigen": "run-s apigen:*",
    "apigen:clean": "rimraf ./src/incremental/api/swagger",
    "apigen:get": "openapi-generator-cli generate -g typescript-fetch -o ./src/incremental/api/swagger -i https://api.swaggerhub.com/apis/OBDASystems/swagger-sparqling_ws/1.0.3",
    "apigen:post-gen": "node src/incremental/api/post-api-gen.js",
    "snapshot": "npm version prerelease --no-git-tag --preid=snap"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/obdasystems/grapholscape.git"
  },
  "keywords": [
    "Ontology",
    "OWL",
    "Graphol"
  ],
  "author": "OBDA Systems",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.18.13",
    "@babel/preset-env": "^7.18.10",
    "@knodes/typedoc-plugin-pages": "^0.23.1",
    "@openapitools/openapi-generator-cli": "^2.6.0",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-commonjs": "^25.0.4",
    "@rollup/plugin-node-resolve": "^15.2.0",
    "@rollup/plugin-replace": "^5.0.2",
    "@rollup/plugin-terser": "^0.4.3",
    "@rollup/plugin-typescript": "^11.1.2",
    "@types/babel__core": "^7.1.20",
    "@types/chroma-js": "^2.4.0",
    "@types/cytoscape": "^3.19.7",
    "@types/jest": "^28.0.8",
    "canvas": "^2.11.2",
    "cpy-cli": "^4.2.0",
    "cross-env": "^7.0.3",
    "gh-pages": "^4.0.0",
    "jest": "^28.0.8",
    "jest-environment-jsdom": "^29.0.1",
    "local-web-server": "^5.2.1",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "rollup": "^3.28.0",
    "rollup-plugin-dts": "^5.3.1",
    "rollup-plugin-import-css": "^3.3.1",
    "rollup-plugin-license": "^3.0.1",
    "ts-jest": "^28.0.8",
    "tslib": "^2.4.1",
    "typedoc": "^0.23.14",
    "typedoc-plugin-extras": "^2.3.0",
    "typedoc-plugin-versions": "^0.2.3",
    "typescript": "^4.8.2"
  },
  "dependencies": {
    "@lit-labs/virtualizer": "^2.0.3",
    "chroma-js": "^2.4.2",
    "cytoscape": "^3.22.1",
    "cytoscape-automove": "^1.10.3",
    "cytoscape-cola": "^2.5.1",
    "cytoscape-edgehandles": "^4.0.1",
    "cytoscape-klay": "^3.1.4",
    "cytoscape-popper": "^2.0.0",
    "cytoscape-svg": "^0.4.0",
    "lit": "^2.3.1",
    "tippy.js": "^6.3.7"
  }
}
