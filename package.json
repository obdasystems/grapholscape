{
  "name": "grapholscape",
  "version": "3.2.2",
  "description": "ontology visualization",
  "module": "dist/grapholscape.esm.js",
  "main": "dist/grapholscape.esm.js",
  "files": [
    "dist"
  ],
  "types": "dist/index.d.ts",
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "scripts": {
    "start": "run-p serve watch",
    "serve": "ws -o -d docs/static/demo",
    "watch": "rollup -c -w",
    "build": "run-p gen-types build:* && run-s clean:types",
    "build:rollup": "cross-env NODE_ENV=production VERSION=$npm_package_version rollup -c",
    "rollup": "rollup -c",
    "clean": "run-s clean:*",
    "clean:dist": "rimraf ./dist",
    "clean:types": "rimraf ./temp",
    "deploy": "cross-env-shell gh-pages -d docs/generated -m \"$npm_package_version\"",
    "doc": "run-s doc:*",
    "doc:typedoc": "typedoc",
    "doc:copy-demo-in-doc": "cpy . ../../generated/demo --cwd=docs/static/demo",
    "test": "jest",
    "gen-types": "run-s clean:types gen-types:tsc",
    "gen-types:tsc": "tsc --declaration --outDir ./temp/types --emitDeclarationOnly",
    "preversion": "run-s test",
    "version": "run-s build doc git-add-all",
    "git-add-all": "git add -A",
    "apigen": "run-s apigen:*",
    "apigen:clean": "rimraf ./src/incremental/api/swagger",
    "apigen:get": "openapi-generator-cli generate -g typescript-fetch -o ./src/incremental/api/swagger -i ./src/incremental/api/api-spec-1.0.1.json"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/obdasystems/grapholscape.git"
  },
  "keywords": [
    "Ontology",
    "OWL",
    "Graphol"
  ],
  "author": "OBDA Systems",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.18.13",
    "@babel/preset-env": "^7.18.10",
    "@knodes/typedoc-plugin-pages": "^0.23.1",
    "@openapitools/openapi-generator-cli": "^2.5.2",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-commonjs": "^22.0.2",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-replace": "^4.0.0",
    "@rollup/plugin-terser": "^0.3.0",
    "@rollup/plugin-typescript": "^8.4.0",
    "@types/babel__core": "^7.1.20",
    "@types/cytoscape": "^3.19.7",
    "@types/jest": "^28.0.8",
    "cpy-cli": "^4.2.0",
    "cross-env": "^7.0.3",
    "gh-pages": "^4.0.0",
    "jest": "^28.0.8",
    "jest-environment-jsdom": "^29.0.1",
    "local-web-server": "^5.2.1",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "rollup": "^2.79.0",
    "rollup-plugin-dts": "^4.2.2",
    "rollup-plugin-license": "^2.8.1",
    "ts-jest": "^28.0.8",
    "tslib": "^2.4.1",
    "typedoc": "^0.23.14",
    "typedoc-plugin-extras": "^2.3.0",
    "typedoc-plugin-versions": "^0.2.3",
    "typescript": "^4.8.2"
  },
  "dependencies": {
    "cytoscape": "^3.22.1",
    "cytoscape-automove": "^1.10.3",
    "cytoscape-cola": "^2.5.1",
    "cytoscape-popper": "^2.0.0",
    "cytoscape-svg": "^0.4.0",
    "lit": "^2.3.1",
    "tippy.js": "^6.3.7"
  }
}
