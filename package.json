{
  "name": "grapholscape",
  "version": "3.0.1",
  "description": "ontology visualization",
  "module": "dist/grapholscape.esm.js",
  "main": "dist/grapholscape.esm.js",
  "files": [
    "dist"
  ],
  "types": "dist/index.d.ts",
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "scripts": {
    "start": "ws -o -d docs/static/demo & rollup -c -w",
    "build": "npm run gen-types & cross-env NODE_ENV=production VERSION=$npm_package_version rollup -c",
    "clean": "rimraf ./dist",
    "app": "rollup -c && electron ./app",
    "app:dist": "electron-packager ./app $npm_package_name --out=./dists --icon=./app/assets/icon --overwrite",
    "deploy": "cross-env-shell gh-pages -d docs/generated -m \"$npm_package_version\"",
    "doc": "npx typedoc && cpy . ../../generated/demo --cwd=docs/static/demo",
    "test": "jest",
    "gen-types": "rimraf ./temp/types && npx tsc --declaration --outDir ./temp/types --emitDeclarationOnly",
    "preversion": "run-s test",
    "version": "run-s build doc app:dist && git add -A"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/obdasystems/grapholscape.git"
  },
  "keywords": [
    "Ontology",
    "OWL",
    "Graphol"
  ],
  "author": "OBDA Systems",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.18.13",
    "@babel/preset-env": "^7.18.10",
    "@knodes/typedoc-plugin-pages": "^0.23.1",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-commonjs": "^22.0.2",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-replace": "^4.0.0",
    "@rollup/plugin-typescript": "^8.4.0",
    "@types/cytoscape": "^3.19.7",
    "@types/jest": "^28.0.8",
    "cpy-cli": "^4.2.0",
    "cross-env": "^7.0.3",
    "electron": "^15.2.0",
    "electron-packager": "^16.0.0",
    "gh-pages": "^4.0.0",
    "jest": "^28.0.8",
    "jest-environment-jsdom": "^29.0.1",
    "local-web-server": "^5.2.1",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "rollup": "^2.79.0",
    "rollup-plugin-dts": "^4.2.2",
    "rollup-plugin-license": "^2.8.1",
    "rollup-plugin-terser": "^7.0.2",
    "ts-jest": "^28.0.8",
    "typedoc": "^0.23.14",
    "typedoc-plugin-extras": "^2.3.0",
    "typescript": "^4.8.2"
  },
  "dependencies": {
    "cytoscape": "^3.22.1",
    "cytoscape-cola": "^2.5.1",
    "cytoscape-popper": "^2.0.0",
    "cytoscape-svg": "^0.4.0",
    "lit": "^2.3.1"
  }
}
