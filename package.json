{
  "name": "grapholscape",
  "version": "3.0.0",
  "description": "ontology visualization",
  "module": "dist/grapholscape.esm.js",
  "main": "dist/grapholscape.esm.js",
  "files": [
    "dist"
  ],
  "types": "dist/index.d.ts",
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "scripts": {
    "start": "ws -o -d doc/static & rollup -c -w",
    "build": "npm run gen-types & cross-env NODE_ENV=production VERSION=$npm_package_version rollup -c",
    "clean": "rimraf ./dist",
    "app": "rollup -c && electron ./app",
    "app:dist": "electron-packager ./app $npm_package_name --out=./dists --icon=./app/assets/icon --overwrite",
    "deploy": "cross-env-shell gh-pages -d doc/output/grapholscape/$npm_package_version -m \"$npm_package_version\"",
    "doc": "",
    "test": "jest",
    "gen-types": "rimraf ./temp/types && npx tsc --declaration --outDir ./temp/types --emitDeclarationOnly",
    "preversion": "run-s test",
    "version": "run-s build doc app:dist && git add -A"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/obdasystems/grapholscape.git"
  },
  "keywords": [
    "Ontology",
    "OWL",
    "Graphol"
  ],
  "author": "OBDA Systems",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.18.0",
    "@babel/preset-env": "^7.16.4",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^21.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.0.5",
    "@rollup/plugin-replace": "^3.0.0",
    "@rollup/plugin-typescript": "^8.3.2",
    "@types/cytoscape": "^3.19.4",
    "@types/jest": "^27.5.1",
    "cross-env": "^7.0.3",
    "electron": "^15.2.0",
    "electron-packager": "^15.4.0",
    "gh-pages": "^3.2.3",
    "jest": "^28.1.0",
    "jest-environment-jsdom": "^28.1.0",
    "local-web-server": "^5.1.1",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "rollup": "^2.58.0",
    "rollup-plugin-dts": "^4.2.2",
    "rollup-plugin-license": "^2.6.0",
    "rollup-plugin-size-snapshot": "^0.12.0",
    "rollup-plugin-terser": "^7.0.2",
    "ts-jest": "^28.0.2",
    "typescript": "^4.6.4"
  },
  "dependencies": {
    "cytoscape": "^3.20.0",
    "cytoscape-cola": "^2.5.0",
    "cytoscape-popper": "^2.0.0",
    "cytoscape-svg": "^0.3.1",
    "lit": "^2.0.2"
  }
}
