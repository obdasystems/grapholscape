{
  "plugins": [ "plugins/markdown", "node_modules/jsdoc-tsimport-plugin/index.js" ],
  "source": {
    "include": [ 
      "src/grapholscape.js",
      "src/model",
      "src/style",
      "src/index.js",
      "src/index.full.js",
      "src/rendering/renderer-manager.js",
      "./package.json",
      "./README.md"
    ]
  },
  "opts": {
    "template": "node_modules/better-docs",
    "destination": "doc/output"
  },
  "templates": {
    "search": true,
    "default": {
        "staticFiles": {
          "include": [
              "./doc/static/"
          ]
        }
    },
    "better-docs": {
      "name": "Grapholscape",
      "logo": "assets/img/icon.png",
      "css": "doc-style.css",
      "navLinks": [
        {
            "label": "Github",
            "href": "https://github.com/obdasystems/grapholscape"
        },
        {
            "label": "Demo",
            "href": "./demo/index.html"
        }
      ]
    }
  }
}